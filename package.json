{
  "name": "note",
  "version": "1.0.0",
  "description": "note with egg.js of typescript",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-note",
    "sequelize": "cross-env sequelize $*",
    "stop": "egg-scripts stop --title=egg-server-note",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "yarn run lint -- --fix && yarn run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "yarn run lint && yarn run cov && yarn run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/validator": "^10.11.3",
    "cross-env": "^6.0.3",
    "crypto": "^1.0.1",
    "egg": "^2.24.0",
    "egg-connector-remote": "^2.0.1",
    "egg-cors": "^2.2.0",
    "egg-i18n": "^2.1.1",
    "egg-passport": "^2.1.0",
    "egg-passport-github": "^1.0.0",
    "egg-passport-local": "^1.2.1",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.11.1",
    "egg-sequelize": "^5.2.0",
    "egg-validate": "^2.0.2",
    "egg-view": "^2.1.2",
    "egg-view-ejs": "^2.0.0",
    "egg-view-nunjucks": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.20.0",
    "sequelize-cli-typescript": "^3.2.0-c",
    "sequelize-typescript": "^1.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "sequelize-cli": "^5.5.1",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "happy",
  "license": "MIT"
}
